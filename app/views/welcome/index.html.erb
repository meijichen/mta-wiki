<div class="container">
  <h1 > Welcome to Career-Wiki </h1>
</div>





<footer>
  <div class = 'footer'>
    <a class="new" href="/categories/new">Create a category</a>
    <a class="new" href="/articles/new">Create a article</a>
  </div>
  <div class='footer category_container'>
    <% Category.all.each do |category| %>
      <div style = "width:<%=100/Category.count%>%;float:left" class='category_index'> <%= link_to category.name, category %></div>
    <% end %>
  </div>
</footer>
